#include <stdio.h>
#include <stdlib.h>

struct money add(struct money a, struct money b);
struct money init(int dollars, int cents);



struct money
{
	int dollars;
	int cents;

};

struct money add(struct money a, struct money b)
{
	money temp;
	temp.dollars =  a.dollars + b.dollars;
	temp.cents = a.cents + b.cents;
	return(temp);

}

struct money init(int dollars, int cents){
	money.dollars = dollars;
	money.cents = cents;


}

void print_money(struct money z)
{
	printf("The Total amount of dollars is %d :", z.dollars);
	printf("The Total amount of cents is %d :", z.cents);

}

int main()
{
	struct money a = init(5,10);
	struct money b,c;
	
	printf("Enter an amount of dollars and cents: ");
	b = init(scanf("%d, %d"));
	c = add(a,b);
	

}














